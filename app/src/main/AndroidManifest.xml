<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.administrator.g2ph">

    <application
        android:allowBackup="true"
        android:icon="@drawable/g2ph_logo_app"
        android:label="@string/app_name"
        android:roundIcon="@drawable/g2ph_logo_app"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NOActionBar">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".IndexActivity" />
        <activity android:name=".WritingActivity" />
        <activity android:name=".CarefulActivity" />
        <activity android:name=".KindsActivity"></activity>
    </application>

</manifest>